{
  "expo": {
    "plugins": [
      "react-native-ble-plx"
    ],
    "name": "Biochallenge",
    "slug": "Biochallenge",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon-meEscuta.png",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./assets/icon-meEscuta.png",
      "resizeMode": "contain",
      "backgroundColor": "#E2C13B"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon-meEscuta.png",
        "backgroundColor": "#E2C13B"
      },
      "permissions": [
        "android.permission.BLUETOOTH",
        "android.permission.BLUETOOTH_ADMIN",
        "android.permission.BLUETOOTH_CONNECT"
      ],
      "package": "com.anonymous.Biochallenge"
    },
    "web": {
      "favicon": "./assets/icon-meEscuta.png"
    }
  }
}
